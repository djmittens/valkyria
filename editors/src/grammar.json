{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "valk",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_form"
      }
    },
    "_form": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "def_form"
        },
        {
          "type": "SYMBOL",
          "name": "fun_form"
        },
        {
          "type": "SYMBOL",
          "name": "lambda_form"
        },
        {
          "type": "SYMBOL",
          "name": "if_form"
        },
        {
          "type": "SYMBOL",
          "name": "do_form"
        },
        {
          "type": "SYMBOL",
          "name": "let_form"
        },
        {
          "type": "SYMBOL",
          "name": "load_form"
        },
        {
          "type": "SYMBOL",
          "name": "sexpr"
        },
        {
          "type": "SYMBOL",
          "name": "qexpr"
        },
        {
          "type": "SYMBOL",
          "name": "quote"
        },
        {
          "type": "SYMBOL",
          "name": "quasiquote"
        },
        {
          "type": "SYMBOL",
          "name": "unquote_splicing"
        },
        {
          "type": "SYMBOL",
          "name": "unquote"
        },
        {
          "type": "SYMBOL",
          "name": "_atom"
        }
      ]
    },
    "def_form": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "STRING",
            "value": "def"
          },
          "named": true,
          "value": "keyword_def"
        },
        {
          "type": "FIELD",
          "name": "binding",
          "content": {
            "type": "SYMBOL",
            "name": "qexpr"
          }
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_form"
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "fun_form": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "STRING",
            "value": "fun"
          },
          "named": true,
          "value": "keyword_fun"
        },
        {
          "type": "FIELD",
          "name": "signature",
          "content": {
            "type": "SYMBOL",
            "name": "qexpr"
          }
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_form"
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "lambda_form": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "STRING",
            "value": "\\"
          },
          "named": true,
          "value": "keyword_lambda"
        },
        {
          "type": "FIELD",
          "name": "params",
          "content": {
            "type": "SYMBOL",
            "name": "qexpr"
          }
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_form"
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "if_form": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "STRING",
            "value": "if"
          },
          "named": true,
          "value": "keyword_if"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_form"
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "do_form": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "STRING",
            "value": "do"
          },
          "named": true,
          "value": "keyword_do"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_form"
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "let_form": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "STRING",
            "value": "="
          },
          "named": true,
          "value": "keyword_let"
        },
        {
          "type": "FIELD",
          "name": "binding",
          "content": {
            "type": "SYMBOL",
            "name": "qexpr"
          }
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_form"
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "load_form": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "STRING",
            "value": "load"
          },
          "named": true,
          "value": "keyword_load"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "sexpr": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_form"
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "qexpr": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_form"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "quote": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "'"
        },
        {
          "type": "SYMBOL",
          "name": "_form"
        }
      ]
    },
    "quasiquote": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "`"
        },
        {
          "type": "SYMBOL",
          "name": "_form"
        }
      ]
    },
    "unquote_splicing": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ",@"
        },
        {
          "type": "SYMBOL",
          "name": "_form"
        }
      ]
    },
    "unquote": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ","
        },
        {
          "type": "SYMBOL",
          "name": "_form"
        }
      ]
    },
    "_atom": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SYMBOL",
          "name": "keyword"
        },
        {
          "type": "SYMBOL",
          "name": "boolean"
        },
        {
          "type": "SYMBOL",
          "name": "nil"
        },
        {
          "type": "SYMBOL",
          "name": "symbol"
        }
      ]
    },
    "number": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": "-"
              },
              {
                "type": "BLANK"
              }
            ]
          },
          {
            "type": "PATTERN",
            "value": "[0-9]+"
          }
        ]
      }
    },
    "string": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "\""
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "PATTERN",
                  "value": "[^\"\\\\]"
                },
                {
                  "type": "SEQ",
                  "members": [
                    {
                      "type": "STRING",
                      "value": "\\"
                    },
                    {
                      "type": "PATTERN",
                      "value": "[abfnrtv\\\\'\"]"
                    }
                  ]
                }
              ]
            }
          },
          {
            "type": "STRING",
            "value": "\""
          }
        ]
      }
    },
    "boolean": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "STRING",
            "value": "true"
          },
          {
            "type": "STRING",
            "value": "false"
          }
        ]
      }
    },
    "nil": {
      "type": "TOKEN",
      "content": {
        "type": "STRING",
        "value": "nil"
      }
    },
    "keyword": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": ":"
          },
          {
            "type": "REPEAT1",
            "content": {
              "type": "PATTERN",
              "value": "[a-zA-Z0-9_+\\-*\\\\/=<>!&?:]"
            }
          }
        ]
      }
    },
    "symbol": {
      "type": "TOKEN",
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "PATTERN",
          "value": "[a-zA-Z0-9_+\\-*\\\\/=<>!&?:]"
        }
      }
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": ";"
          },
          {
            "type": "PATTERN",
            "value": "[^\\n]*"
          }
        ]
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}