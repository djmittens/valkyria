{
  "name": "tree-sitter-valk",
  "version": "0.1.0",
  "description": "Tree-sitter grammar for the Valkyria Lisp language",
  "main": "bindings/node",
  "keywords": ["tree-sitter", "parser", "lisp", "valkyria"],
  "dependencies": {
    "node-addon-api": "^8.0.0",
    "node-gyp-build": "^4.8.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.25.0"
  },
  "tree-sitter": [
    {
      "scope": "source.valk",
      "file-types": ["valk"],
      "highlights": "queries/highlights.scm",
      "injection-regex": "^valk$"
    }
  ]
}
