; Test without loading prelude to see builtin behavior
(printf "Test 1: head on list\n")
(= {mylist} {100 200 300})
(= {result} (head mylist))
(printf "Type: %ld, Len: %ld\n" (type result) (len result))
(print result)

(printf "\nTest 2: Can we use join to unpack?\n")
(= {unpacked} (join (head mylist)))
(printf "Type: %ld, Len: %ld\n" (type unpacked) (len unpacked))
(print unpacked)

