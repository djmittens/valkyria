; Direct test without loading modules
(printf "Starting test...\n")

; Test head and tail directly
(= {test-list} {a b c})
(printf "Original list: {a b c}\n")
(printf "Head result: ")
(print (head test-list))
(printf "Tail result: ")
(print (tail test-list))

; Test with map function (using head/tail repeatedly)
(fun {my-map f l} {
  if (== l {})
    {{}}
    {join (list (f (eval (head l)))) (my-map f (tail l))}
})

(printf "\nTesting map with head/tail:\n")
(= {nums} {1 2 3 4 5})
(fun {double x} {* x 2})
(printf "Doubling {1 2 3 4 5}: ")
(print (my-map double nums))

(printf "\nTests completed!\n")