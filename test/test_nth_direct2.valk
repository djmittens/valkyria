(load "src/prelude.valk")

(printf "Testing nth directly:\n")
(= {mylist} {10 20 30})
(printf "List: ")
(print mylist)

(printf "\nTrying fst:\n")
(= {result-fst} (fst mylist))
(printf "fst result: ")
(print result-fst)

(printf "\nTrying nth 1:\n")
(= {result-nth} (nth 1 mylist))
(printf "nth 1 result: ")
(print result-nth)

(printf "\nTesting head:\n")
(= {result-head} (head mylist))
(printf "head result: ")
(print result-head)

(printf "\nTesting eval head:\n")
(= {result-eval-head} (eval (head mylist)))
(printf "eval head result: ")
(print result-eval-head)

(printf "\nTesting tail:\n")
(= {result-tail} (tail mylist))
(printf "tail result: ")
(print result-tail)

(shutdown 0)
