; Test different body types

; Body with explicit sexpr
(def {test1} (\ {x} {(+ x 1)}))
(print "test1 10:")
(print (test1 10))

; Body with implicit sexpr (multi-element qexpr)
(def {test2} (\ {x} {+ x 1}))
(print "test2 10:")
(print (test2 10))

; Body with single symbol
(def {test3} (\ {x} {x}))
(print "test3 42:")
(print (test3 42))
