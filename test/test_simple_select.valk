(load "src/prelude.valk")

(printf "Test 1: Simple value\n")
(= {x} 42)
(printf "x = %ld\n" x)

(printf "\nTest 2: Call function with params\n")
(fun {add a b} {+ a b})
(printf "add 10 20 = %ld\n" (add 10 20))

(printf "\nTest 3: fst/snd\n")
(printf "fst {100 200}: %ld\n" (fst {100 200}))
(printf "snd {100 200}: %ld\n" (snd {100 200}))

(shutdown 0)
