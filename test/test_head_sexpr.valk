(printf "Testing head on Q-expr containing S-expr:\n\n")

(= {q} {(+ 1 2) (+ 3 4)})
(printf "Q-expr: ")
(print q)

(printf "\nhead of Q-expr: ")
(= {h} (head q))
(print h)

(printf "\nWhat is h? Let's check its elements\n")
(printf "Type of h: %ld\n" (type h))

(printf "\neval of h: ")
(= {evaled} (eval h))
(print evaled)

(printf "\nNow using unpack:\n")
(fun {id x} {x})
(= {unpacked} (unpack id h))
(printf "unpack id h: ")
(print unpacked)

(shutdown 0)
