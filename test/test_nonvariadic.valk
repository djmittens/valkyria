; Non-variadic function test
(printf "Before defining test-func\n")

(def {test-func}
  (fn {x y} {
    (printf "Inside test-func, x = %s, y = %s\n" x y)
    (+ x y)
  })
)

(printf "After defining test-func\n")
(printf "About to call test-func\n")

(def {result} (test-func 1 2))

(printf "After calling test-func, result = %s\n" result)
