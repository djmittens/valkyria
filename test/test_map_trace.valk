(load "src/prelude.valk")

(fun {test-scoping n} {
  do
    (printf "n inside function: %ld\n" n)
    (= {branch} {(== n 1) 100})
    (printf "Branch: ")
    (print branch)
    (= {cond} (fst branch))
    (printf "Condition: %ld\n" cond)
})

(printf "Calling test-scoping:\n")
(test-scoping 1)

(shutdown 0)
