(load "src/prelude.valk")

(printf "Test 1: nth on simple list\n")
(= {mylist} {100 200 300})
(= {first} (nth 1 mylist))
(printf "First: %ld\n" first)

(printf "\nTest 2: nth with 1-based indexing\n")
(printf "nth 1: %ld\n" (nth 1 {10 20 30}))
(printf "nth 2: %ld\n" (nth 2 {10 20 30}))
(printf "nth 3: %ld\n" (nth 3 {10 20 30}))

(printf "\nTest 3: nth on nested list\n")
(= {nested} {{a b} {c d}})
(= {item} (nth 1 nested))
(printf "Item type: %ld, len: %ld\n" (type item) (len item))
(print item)

(shutdown 0)
