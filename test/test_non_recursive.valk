(load "src/prelude.valk")

(print "Test non-recursive function that calls another")
(fun {helper x} {* 2 x})
(fun {caller x} {helper (+ x 1)})

(print "Calling caller 5")
(print (caller 5))

(print "Test function that calls builtin in if")
(fun {test-builtin x} {
  if (== x 0)
    {999}
    {+ x 100}
})

(print "Calling test-builtin 0")
(print (test-builtin 0))
(print "Calling test-builtin 5")
(print (test-builtin 5))
