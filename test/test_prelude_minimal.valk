(print "1. Loading print statement")
(print "Hello and welcome to the prelude n Shit")

(print "2. Defining fun macro")
(def {fun} (\ {f b}  {
   def (head f) (\ (tail f) b)
}))

(print "3. Defining nil")
(def {nil} {})

(print "4. Defining true")
(def {true} 1)

(print "5. Defining false")
(def {false} 0)

(print "6. Testing fun macro with unpack")
(fun {unpack f l} {
     eval (join (list f) l)
})

(print "7. Testing unpack")
(print (unpack + {1 2 3}))

