(load "src/prelude.valk")

(printf "Testing parameter binding:\n")

(fun {test x} {
  (printf "In test, x = ")
  (print x)
  (printf "Type of x: %ld\n" (type x))
  (printf "Evaluating x: ")
  (= {evaled} (eval x))
  (print evaled)
  x
})

(= {lst} {10 20 30})
(printf "\nCalling test with list:\n")
(test lst)

(printf "\nDone\n")
(shutdown 0)
