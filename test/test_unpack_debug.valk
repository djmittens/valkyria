; Debug unpack
(print "Step 1: list function")
(print (list + {1 2 3}))

(print "Step 2: join")
(print (join (list +) {1 2 3}))

(print "Step 3: eval")
(print (eval (join (list +) {1 2 3})))

(print "Step 4: as a function")
(def {myunpack} (\ {f l} {
  (print "Inside myunpack, f =")
  (print f)
  (print "l =")
  (print l)
  (print "Calling join:")
  (def {joined} (join (list f) l))
  (print joined)
  (print "Calling eval:")
  (eval joined)
}))

(print "Calling myunpack:")
(print (myunpack + {1 2 3}))
