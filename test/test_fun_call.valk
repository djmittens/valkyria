; Test if the fun macro actually works
(def {fun} (\ {f b} {
   def (head f) (\ (tail f) b)
}))

(print "Calling fun")
(def {result} (fun {test x} {
  (print "Inside test, x =")
  (print x)
}))

(print "Result of fun:")
(print result)

(print "Looking up test:")
(print test)

(print "Calling test")
(test 42)
(print "Done")
