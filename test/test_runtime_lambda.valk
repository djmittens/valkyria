; Test runtime lambda creation with computed formals/body
(def {f-list} {add a b})
(def {b-expr} {+ a b})

(print "f-list:")
(print f-list)

(print "head f-list:")
(print (head f-list))

(print "tail f-list:")
(print (tail f-list))

(print "Creating lambda with computed args:")
(def {my-lambda} (\ (tail f-list) b-expr))
(print "Lambda created:")
(print my-lambda)

(print "Calling lambda:")
(print (my-lambda 10 20))
