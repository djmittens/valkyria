; Test what head should return without prelude
(printf "Testing head behavior:\n")

(= {lst1} {a b c})
(printf "List: ")
(print lst1)
(printf "head: ")
(print (head lst1))

(= {lst2} {10})
(printf "\nList: ")
(print lst2) 
(printf "head: ")
(print (head lst2))

(printf "\nWhat should head return?\n")
(printf "head of {a b c} should return {a}\n")
(printf "head of {10} should return {10}\n")

(shutdown 0)
